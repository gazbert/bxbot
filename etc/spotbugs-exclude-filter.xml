<?xml version="1.0"?>

<!--
  Bug codes can be found here: http://findbugs.sourceforge.net/bugDescriptions.html
  错误代码可以在这里找到：http://findbugs.sourceforge.net/bugDescriptions.html
 -->
<FindBugsFilter>

    <!-- Don't bother with unused fields in Exchange Adapter GSON classes.
     不要打扰 Exchange 适配器 GSON 类中未使用的字段。-->
    <Match>
        <Bug code="UwF,NP"/>
    </Match>

    <!-- False positive - this NPE check is required. 误报 - 此 NPE 检查是必需的。 -->
    <Match>
        <Class name="com.gazbert.bxbot.exchanges.ItBitExchangeAdapter"/>
        <Bug code="RCN"/>
    </Match>

    <!-- False positive - this NPE check is required. 误报 - 此 NPE 检查是必需的。 -->
    <Match>
        <Class name="com.gazbert.bxbot.datastore.xml.ConfigurationManager"/>
        <Bug code="OBL"/>
    </Match>

    <!-- Ignore default interface method warning  忽略默认接口方法警告-->
    <Match>
        <Class name="com.gazbert.bxbot.trading.api.TradingApi"/>
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON"/>
    </Match>

    <!-- Ignore static inner warnings for Kraken adapter 忽略 Kraken 适配器的静态内部警告-->
    <Match>
        <Class name="com.gazbert.bxbot.exchanges.KrakenExchangeAdapter"/>
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON"/>
    </Match>

    <!-- Ignore Exception not thrown for catch warnings in Kraken adapter Kraken适配器中的catch警告未引发忽略异常 -->
    <Match>
        <Class name="com.gazbert.bxbot.exchanges.KrakenExchangeAdapter"/>
        <Bug pattern="REC_CATCH_EXCEPTION"/>
    </Match>

    <!-- Ignore Exception not thrown for catch warnings in Gemini adapter Gemini 适配器中的捕获警告未引发忽略异常 -->
    <Match>
